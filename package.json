{
    "name": "ezgesture",
    "version": "1.1.0",
    "description": "Easily add gestures functionality with simple native DOM events",
    "exports": {
        ".": "./dist/index.js",
        "./pinch": "./dist/pinch.js",
        "./drag": "./dist/drag.js",
        "./longpress": "./dist/longpress.js"
    },
    "types": "types/",
    "scripts": {
        "dev": "rollup -cw",
        "build": "rollup -c",
        "copy-dist": "mkdir -p demos/dist && cp -r dist/ demos/",
        "publish-ghp": "git subtree push --prefix demos origin gh-pages",
        "prepublishOnly": "rollup -c"
    },
    "files": [
        "lib/*",
        "dist/*",
        "README.md",
        "LICENSE"
    ],
    "keywords": [
        "gesture",
        "pinch",
        "drag",
        "move",
        "mouse",
        "touch",
        "dom",
        "event",
        "interaction"
    ],
    "devDependencies": {
        "@rollup/plugin-typescript": "^8.2.1",
        "rollup": "^2.41.5",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.30.0",
        "terser": "^5.6.0",
        "tslib": "^2.2.0",
        "typescript": "^4.2.4"
    },
    "author": "Mohammed Al-Qurafi",
    "license": "MIT",
    "homepage": "https://github.com/mhmd-22/ezgesture#readme",
    "bugs": {
        "url": "https://github.com/mhmd-22/ezgesture/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mhmd-22/ezgesture.git"
    }
}
